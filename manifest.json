{
    "manifest_version": 2,
    "name": "bruh.js",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persstent": false
    },
    "permissions": ["storage"],
    "browser_action": {
        "default_popup": "popup.html"
    }
}